{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
		[
			"module-resolver",
			{
				"cwd": "babelrc",
				"root": ["./src"],
				"extensions": [".js", ".ios.js", ".android.js"],
				"alias": {
					"@screen": "./src/screen",
					"@sagas": "./src/sagas",
					"@reducers": "./src/reducers",
					"@store": "./src/store",
					"@assets": "./src/assets",
					"@constants": "./src/constants",
					"@validators": "./src/validators",
					"@api": "./src/api"
				}
			}
		]
	]
}
